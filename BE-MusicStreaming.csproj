    <UserSecretsId>773b2354-d13d-4504-b4b4-4e43cb3469f2</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.2" />
